{
  "private": true,
  "name": "announcement",
  "version": "1.0.0",
  "scripts": {
    "start": "dumi dev",
    "build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\""
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "qrcode": "^1.4.4",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@types/qrcode": "^1.3.5",
    "dumi": "^1.0.10",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1"
  }
}
